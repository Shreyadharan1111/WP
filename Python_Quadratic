import math 
a,b,c = map(int,input("enter a,b,c").split())
d = pow(b,2) - (4*a*c)
if d > 0 :
    r1 = (-b + math.sqrt(d))/(2*a)
    r2 = (-b - math.sqrt(d))/(2*a)
    print(f"The roorts are {r1} and {r2} ")
else :
    if d == 0 :
        r1 = -b/(2*a)
        r2 = r1 
        print(f"The roorts are {r1} and {r2} ")

    print("roots are imaginary")

